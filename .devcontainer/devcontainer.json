{
  "name": "mitmproxy-dump",
  "image": "mcr.microsoft.com/devcontainers/python:3.10",

  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "ms-python.vscode-pylance"
      ]
    }
  },

  "postCreateCommand": "pip install -r requirements.txt",

  "forwardPorts": [
    8080
  ],

  "settings": {
    "terminal.integrated.defaultProfile.linux": "bash"
  }
}